@page "/auctionhouse"

<PageTitle>Auction House</PageTitle>

<h3>Live Auctions</h3>

<p>View of the live auctions on Auctionator</p>


<button type="button">Post new auction</button>



@if (items == null)
{
    <p><em>Loading...</em></p>
}
else
{
<table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Tentative (y/n)</th>
                <th>Launch Date</th>
                <th>Mission Name</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var launch in launches)
            {
                <tr>
                    <td>@launch.Id</td>
                    <td>@launch.IsTentative</td>
                    <td>@launch.LaunchDateLocal</td>
                    <td>@launch.MissionName</td>
                </tr>
            }
        </tbody>
    </table>
}